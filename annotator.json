{
    "steps": [
        "bert_title",
        "bert_text",
        "rvs_fasttext_title",
        "rvs_elmo_title"
    ],
    "processors": {
        "bert_client": {
            "type": "bert_client",
            "ip": "localhost",
            "port": 5555,
            "port_out": 5556
        },
        "rvs_fasttext": {
            "type": "fasttext",
            "path": "/media/yallen/My Passport/Models/rvs_taiga_fasttext/model.model"
        },
        "rvs_elmo": {
            "type": "elmo",
            "options_file": "/media/yallen/My Passport/Models/rvs_elmo/options.json",
            "weight_file": "/media/yallen/My Passport/Models/rvs_elmo/model.hdf5",
            "cuda_device": 0
        }
    },
    "bert_title": {
        "processor": "bert_client",
        "input_field": "title",
        "output_field": "title_bert_embedding"
    },
    "bert_text": {
        "processor": "bert_client",
        "input_field": "text",
        "output_field": "text_bert_embedding"
    },
    "rvs_fasttext_title": {
        "processor": "rvs_fasttext",
        "input_field": "title",
        "output_field": "title_rvs_fasttext_embedding"
    },
    "rvs_elmo_title": {
        "processor": "rvs_elmo",
        "input_field": "title",
        "output_field": "title_rvs_elmo_embedding"
    }
}
